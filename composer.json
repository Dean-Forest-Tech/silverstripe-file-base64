{
  "name": "dft/silverstripe-file-base64",
  "description": "Simple module to add base64 string data methods to SilverStripe files and images",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/Dean-Forest-Tech/silverstripe-file-base64",
    "keywords": [
        "silverstripe",
        "files",
        "images",
        "Base64"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dean Forest Tech",
            "homepage": "https://dean-forest-tech.uk"
        }
    ],
    "support": {
        "issues": "http://github.com/Dean-Forest-Tech/silverstripe-file-base64/issues"
    },
    "require": {
        "silverstripe/framework": "^4|^5"
    },
    "autoload": {
        "psr-4": {
            "DFT\\SilverStripe\\FileBase64\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
